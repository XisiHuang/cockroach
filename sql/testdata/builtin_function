query T
SELECT SUBSTR('RoacH', 2)
----
oacH

query T
SELECT SUBSTR('RoacH', -2)
----
RoacH

query T
SELECT SUBSTR('RoacH', -2, 4)
----
R

query T
SELECT SUBSTR('12345', 2, 77)
----
2345

query T
SELECT SUBSTR('12345', -2, 77)
----
12345

statement error SUBSTR: invalid argument: negative substring length not allowed: -1
SELECT SUBSTR('12345', 2, -1)
